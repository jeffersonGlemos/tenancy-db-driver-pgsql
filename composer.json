{
    "name": "fletch3555/tenancy-db-driver-pgsql",
    "description": "The tenancy/tenancy database driver for pgsql",
    "keywords": ["database", "tenancy", "pgsql", "postgresql"],
    "license": "MIT",
    "require": {
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0",
        "tenancy/hooks-database": "^3.0.3",
        "tenancy/affects-connections": "*",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.7",
        "laravel/laravel": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.0",
        "tenancy/hooks-migration": "*",
        "tenancy/testing": "*"
    },
    "autoload": {
        "psr-4": {
            "Tenancy\\Database\\Drivers\\Pgsql\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenancy\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Eric Fletcher",
            "email": "eric@thefletcher.net",
            "homepage": "https://thefletcher.net"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Tenancy\\Database\\Drivers\\Pgsql\\Provider"
            ]
        }
    }
}
